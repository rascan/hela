{
    "name": "rascan/hela",
    "description": "Laravel wrapper around Safaricom's Daraja service",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank",
            "email": "franciskisiara@gmail.com",
            "homepage": "https://hela.rascan.space"
        }
    ],
    "homepage": "https://github.com/rascan/hela",
    "keywords": ["Laravel", "Hela"],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.17|^9.0|^10.0",
        "illuminate/validation": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Rascan\\Hela\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rascan\\Hela\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rascan\\Hela\\HelaServiceProvider"
            ],
            "aliases": {
                "Hela": "Rascan\\Hela\\Facades\\Hela"
            }
        }
    }
}
